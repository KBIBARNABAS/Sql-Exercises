{
	"name": "certificationdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "rawpicker",
						"type": "DatasetReference"
					},
					"name": "certification"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sendtransformed",
						"type": "DatasetReference"
					},
					"name": "certificationsink"
				}
			],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> certification",
				"certification derive(each(match(name=='CertificationID'), $$ = toLong($$)),",
				"          each(match(name=='CertificationID'), $$ = toLong($$)),",
				"          each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='Course_ID'), $$ = toLong($$)),",
				"          each(match(name=='Course_ID'), $$ = toLong($$)),",
				"          each(match(name=='CompletionDuration'), $$ = toInteger($$)),",
				"          each(match(name=='CompletionDuration'), $$ = toInteger($$))) ~> ModifyColumns1",
				"ModifyColumns1 select(mapColumn(",
				"          each(match(name!='TRIAL642'))",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['certificationTransformed'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> certificationsink"
			]
		}
	}
}