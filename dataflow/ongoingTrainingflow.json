{
	"name": "ongoingTrainingflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "rawpicker",
						"type": "DatasetReference"
					},
					"name": "ongoingTraining"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sendtransformed",
						"type": "DatasetReference"
					},
					"name": "ongoingTrainingTransformed"
				}
			],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> ongoingTraining",
				"ongoingTraining derive(each(match(name=='TrainingID'), $$ = toLong($$)),",
				"          each(match(name=='TrainingID'), $$ = toLong($$)),",
				"          each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='Course_ID'), $$ = toLong($$)),",
				"          each(match(name=='Course_ID'), $$ = toLong($$)),",
				"          each(match(name=='Status'), $$ = toInteger($$)),",
				"          each(match(name=='Status'), $$ = toInteger($$)),",
				"          each(match(name=='CompletionPercentage'), $$ = toInteger($$)),",
				"          each(match(name=='CompletionPercentage'), $$ = toInteger($$))) ~> ModifyColumns1",
				"ModifyColumns1 select(mapColumn(",
				"          each(match(name!='TRIAL661'))",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['ongoingTrainingTransformed'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> ongoingTrainingTransformed"
			]
		}
	}
}