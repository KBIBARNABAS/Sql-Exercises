{
	"name": "reviewdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "rawpicker",
						"type": "DatasetReference"
					},
					"name": "review"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sendtransformed",
						"type": "DatasetReference"
					},
					"name": "reviewTransformed"
				}
			],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> review",
				"review derive(each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='UserID'), $$ = toInteger($$)),",
				"          each(match(name=='CourseID'), $$ = toLong($$)),",
				"          each(match(name=='CourseID'), $$ = toLong($$)),",
				"          each(match(name=='Likes'), $$ = toBoolean($$)),",
				"          each(match(name=='Likes'), $$ = toBoolean($$)),",
				"          each(match(name=='RankingScore'), $$ = toInteger($$)),",
				"          each(match(name=='RankingScore'), $$ = toInteger($$))) ~> ModifyColumns1",
				"ModifyColumns1 select(mapColumn(",
				"          each(match(name!='TRIAL691'))",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['reviewTransformed'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> reviewTransformed"
			]
		}
	}
}